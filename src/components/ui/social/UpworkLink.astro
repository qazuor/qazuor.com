---
import SocialLink from './SocialLink.astro';

interface Props {
  href: string;
  ariaLabel?: string;
  size?: 'sm' | 'md' | 'lg';
  className?: string;
}

const { href, ariaLabel = 'Upwork', size = 'md', className = '' } = Astro.props;
---

<SocialLink href={href} ariaLabel={ariaLabel} iconName="upwork" size={size} className={className} />
